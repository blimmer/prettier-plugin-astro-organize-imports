{
  "name": "prettier-plugin-astro-organize-imports",
  "version": "0.0.1-alpha.2",
  "description": "A Prettier plugin for formatting imports in Astro files",
  "keywords": [
    "plugin",
    "formatter",
    "prettier",
    "astro",
    "sort-imports"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ot07/prettier-plugin-astro-organize-imports.git"
  },
  "homepage": "https://github.com/ot07/prettier-plugin-astro-organize-imports#readme",
  "bugs": {
    "url": "https://github.com/ot07/prettier-plugin-astro-organize-imports/issues"
  },
  "license": "MIT",
  "author": "ot07",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "_pre": "rimraf dist",
    "_esbuild": "node build.js",
    "prebuild": "npm run _pre",
    "build": "npm run _esbuild -- --minify",
    "predev": "npm run _pre",
    "dev": "npm run _esbuild",
    "format": "prettier \"src/**/*.js\" --write"
  },
  "dependencies": {
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "esbuild": "^0.18.10",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.15.0"
  }
}
